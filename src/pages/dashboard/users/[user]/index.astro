---
import DashboardLayout from "@/layouts/DashboardLayout.astro";
import NewUser from "./_components/NewUser";

const { permissions, user } = Astro.locals;
if (!permissions?.includes("Users_Write") && user?.userType !== "admin") {
  return Astro.redirect("/dashboard/no-access", 302);
}

---

<DashboardLayout title='admin'>
    <NewUser client:only></NewUser>
</DashboardLayout>
